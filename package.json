{
  "name": "rothschild-boilerplate-back-end",
  "version": "0.0.1",
  "scripts": {
    "start": "node bin/www",
    "dev":
      "concurrently  \"PORT=3001 nodemon bin/www --ignore client\" \"cd client && npm start \" ",
    "heroku-postbuild":
      "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": ["node_modules", "client/node_modules"],

  "description": "GraphQL backend ",

  "engines": {
    "node": ">= 7.6.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.1",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "express": "~4.15.2",
    "express-graphql": "^0.6.11",
    "express-session": "^1.15.6",
    "graphql": "0.11.7",
    "html-to-text": "^3.3.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.0",
    "juice": "^4.2.0",
    "mongoose": "^4.12.1",
    "morgan": "~1.9.0",
    "nodemailer": "^4.1.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-url": "^1.0.4",
    "promisify": "0.0.3",

    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "1.7.4",
    "react-scripts": "^1.0.14"
  }
}
